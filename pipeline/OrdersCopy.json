{
	"name": "OrdersCopy",
	"properties": {
		"activities": [
			{
				"name": "SqlJson2BlobText",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [
					{
						"name": "Source",
						"value": "."
					},
					{
						"name": "Destination",
						"value": "storedb//"
					}
				],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": "SELECT [value] FROM OPENJSON(\r\n  (SELECT\r\n    o.OrderID,\r\n    o.OrderDate,\r\n    o.FirstName,\r\n    o.LastName,\r\n    o.Address,\r\n    o.City,\r\n    o.State,\r\n    o.PostalCode,\r\n    o.Country,\r\n    o.Phone,\r\n    o.SMSOptin,\r\n    o.SMSStatus,\r\n    o.Email,\r\n    o.ReceiptUrl,\r\n    o.Total,\r\n    (select OrderDetailId, ProductId, UnitPrice, Quantity from OrderDetails od where od.OrderId = o.OrderId for json auto) as OrderDetails\r\n   FROM Orders o FOR JSON PATH)\r\n)",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"validateDataConsistency": false
				},
				"inputs": [
					{
						"referenceName": "SourceDataset_97z",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "DestinationDataset_97z",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "BlobJsonToCosmos",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "SqlJson2BlobText",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobStorageReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"sink": {
						"type": "CosmosDbSqlApiSink",
						"writeBehavior": "insert"
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Json1",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CosmosDbSqlApiCollection1",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}